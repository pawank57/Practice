package org.cybage;


class A {
	public void sum(int a, int b) {
//		System.out.println(a + b);
	}
}

class B extends A{
	public void sum(double a, double b) {
//		System.out.println(a + b);
	}
}

public class Program {
	public static void main(String[] args) {
		B a = new B();
		a.sum(10.20, 20.30);
	}
}
